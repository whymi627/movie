{
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "overrides": [
      {
          "files": "*.js",
          "options": {
              "parser": "flow"
          }
      },
      {
          "files": ".prettierrc",
          "options": {
              "parser": "json"
          }
      }
  ]
}